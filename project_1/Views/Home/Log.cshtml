
@model List<LogModel>

<table>
    <tr>
        <th>Дата и время</th>
        <th>Уровень</th>
        <th>Сообщение</th>
    </tr>
    @foreach (var log in Model)
    {
        <tr>
            <td>@log.DateTime</td>
            <td>@log.Level</td>
            <td>@log.Message</td>
        </tr>
    }
</table>

@if (User.IsInRole("admin"))
{
    <p>Вы являетесь администратором</p>
}
